@model Shaligram_Recruitment.Model.ViewModels.Student.StudentModel

<dl class="row"> 
    <dt class="col-sm-10 ms-3 fs-5" style="color: #7A8595;">Student Name:</dt>
    <dd class="col-sm-10 ms-3 fs-5" style="color: #333333; font:bold">@Model.StudentName</dd>
    <dt class="col-sm-10 ms-3 fs-5" style="color: #7A8595;">College Name:</dt>
    <dd class="col-sm-10 ms-3 fs-5" style="color: #333333; font:bold ">@Model.CollegeName</dd>
    <dt class="col-sm-10 ms-3 fs-5" style="color: #7A8595;">Batch Year:</dt>
    <dd class="col-sm-10 ms-3 fs-5" style="color: #333333; font:bold ">@Model.BatchYear</dd>
    <dt class="col-sm-10 ms-3 fs-5" style="color: #7A8595;">Email Address:</dt>
    <dd class="col-sm-10 ms-3 fs-5" style="color: #333333; font:bold ">@Model.EmailAddress</dd>
    <dt class="col-sm-10 ms-3 fs-5" style="color: #7A8595;">Phone Number:</dt>
    <dd class="col-sm-10 ms-3 fs-5" style="color: #333333; font:bold ">@Model.PhoneNumber</dd>
</dl>
